    <span class="ruby-identifier">line</span> = <span class="ruby-identifier">pd</span>.<span class="ruby-identifier">next_line</span>
    
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value">3</span>,         <span class="ruby-identifier">line</span>.<span class="ruby-identifier">keys</span>.<span class="ruby-identifier">size</span>)
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value str">'doug'</span>,    <span class="ruby-identifier">line</span>[<span class="ruby-value str">'first'</span>])
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value str">'tolton'</span>,  <span class="ruby-identifier">line</span>[<span class="ruby-value str">'last'</span>])
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value str">'32'</span>,      <span class="ruby-identifier">line</span>[<span class="ruby-value str">'age'</span>])
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value">1</span>,         <span class="ruby-identifier">pd</span>.<span class="ruby-identifier">line_number</span>)
    
    <span class="ruby-identifier">line</span> = <span class="ruby-identifier">pd</span>.<span class="ruby-identifier">next_line</span>
    
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value">3</span>,         <span class="ruby-identifier">line</span>.<span class="ruby-identifier">keys</span>.<span class="ruby-identifier">size</span>)
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value str">'torrey'</span>,  <span class="ruby-identifier">line</span>[<span class="ruby-value str">'first'</span>])
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value str">'tolton'</span>,  <span class="ruby-identifier">line</span>[<span class="ruby-value str">'last'</span>])
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value str">'30'</span>,      <span class="ruby-identifier">line</span>[<span class="ruby-value str">'age'</span>])
    <span class="ruby-identifier">assert_equal</span>(<span class="ruby-value">2</span>,         <span class="ruby-identifier">pd</span>.<span class="ruby-identifier">line_number</span>)
    
